{
  "name": "barrhawk-premium-e2e",
  "version": "1.0.0-beta.2",
  "description": "BarrHawk Premium E2E Testing - Self-healing MCP server with tripartite architecture",
  "type": "module",
  "main": "packages/supervisor/index.ts",
  "scripts": {
    "beta": "bun run packages/supervisor/primary/index.ts",
    "beta:secondary": "bun --hot run packages/supervisor/secondary/index.ts",
    "beta:test": "bun run packages/supervisor/scripts/test-beta.ts",
    "mcp:test": "bun run packages/supervisor/scripts/test-mcp.ts",
    "test": "bun run beta:test && bun run mcp:test",
    "tripartite": "cd tripartite && ./start.sh",
    "tripartite:bridge": "bun run tripartite/bridge/index.ts",
    "tripartite:doctor": "bun run tripartite/doctor/index.ts",
    "tripartite:igor": "bun run tripartite/igor/index.ts",
    "tripartite:frank": "bun run tripartite/frankenstein/index.ts",
    "mcp:frank": "bun run tripartite/mcp-frank.ts"
  },
  "keywords": [
    "mcp",
    "e2e",
    "testing",
    "ai",
    "self-healing",
    "automation",
    "tripartite",
    "swarm"
  ],
  "author": "BarrHawk",
  "license": "MIT",
  "engines": {
    "bun": ">=1.1.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0"
  }
}
